<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".activityAdmin.HomeAdminActivity">

    <ScrollView
        android:id="@+id/mainLayout"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:fillViewport="true"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">

            <androidx.constraintlayout.widget.ConstraintLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent">

                <androidx.constraintlayout.widget.ConstraintLayout
                    android:id="@+id/constraintLayout2"
                    android:layout_width="match_parent"
                    android:layout_height="150dp"
                    android:background="#ECEBEB"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent">

                    <ImageView
                        android:id="@+id/imgLogout"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="20dp"
                        android:layout_marginEnd="28dp"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintTop_toTopOf="parent"
                        app:srcCompat="@drawable/baseline_logout_24" />

                    <TextView
                        android:id="@+id/txtNamaHalo"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="28dp"
                        android:layout_marginTop="16dp"
                        android:fontFamily="@font/andika"
                        android:text="Halo, Nama"
                        android:textColor="#000000"
                        android:textSize="16sp"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toTopOf="parent" />

                    <TextView
                        android:id="@+id/txtLogout"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="16dp"
                        android:layout_marginEnd="2dp"
                        android:fontFamily="@font/andika"
                        android:text="Logout"
                        android:textColor="#000000"
                        android:textSize="16sp"
                        app:layout_constraintEnd_toStartOf="@+id/imgLogout"
                        app:layout_constraintTop_toTopOf="parent" />
                </androidx.constraintlayout.widget.ConstraintLayout>

                <androidx.constraintlayout.widget.ConstraintLayout
                    android:id="@+id/constraintLayout4"
                    android:layout_width="match_parent"
                    android:layout_height="200dp"
                    android:layout_marginTop="-90dp"
                    android:background="@drawable/corner_top_noborder"
                    android:backgroundTint="#97F2F1"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/constraintLayout5" />

                <androidx.constraintlayout.widget.ConstraintLayout
                    android:id="@+id/constraintLayout5"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="30dp"
                    android:layout_marginTop="-100dp"
                    android:layout_marginEnd="30dp"
                    android:background="@drawable/corner_image"
                    android:backgroundTint="#FFFFFF"
                    app:layout_constraintBottom_toTopOf="@+id/constraintLayout4"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/constraintLayout2">

                    <LinearLayout
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:orientation="vertical"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintStart_toEndOf="@+id/imageprofiledokter"
                        app:layout_constraintTop_toBottomOf="@+id/tv_profilenamapasien">

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:orientation="horizontal">

                            <ImageView
                                android:id="@+id/imageView22"
                                android:layout_width="30dp"
                                android:layout_height="30dp"
                                android:layout_marginTop="2dp"
                                app:srcCompat="@drawable/internet" />

                            <TextView
                                android:id="@+id/tv_hpadmin"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_marginStart="5dp"
                                android:fontFamily="@font/andika"
                                android:text="089618876128"
                                android:textColor="#000000"
                                android:textSize="16sp" />

                        </LinearLayout>

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:orientation="horizontal">

                            <ImageView
                                android:id="@+id/imageView18"
                                android:layout_width="30dp"
                                android:layout_height="30dp"
                                android:layout_marginTop="2dp"
                                app:srcCompat="@drawable/email__1_" />

                            <TextView
                                android:id="@+id/tv_emailadmin"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_marginStart="5dp"
                                android:fontFamily="@font/andika"
                                android:text="test123@gmail.com"
                                android:textColor="#000000"
                                android:textSize="16sp" />
                        </LinearLayout>

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:orientation="horizontal">

                            <ImageView
                                android:id="@+id/imageView25"
                                android:layout_width="30dp"
                                android:layout_height="30dp"
                                android:layout_marginTop="2dp"
                                app:srcCompat="@drawable/man__1_" />

                            <TextView
                                android:id="@+id/tv_profilegenderadmin"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginStart="5dp"
                                android:fontFamily="@font/andika"
                                android:text="Pria"
                                android:textColor="#000000"
                                android:textSize="16sp" />

                        </LinearLayout>

                        <TextView
                            android:id="@+id/txtDetailProfileAdmin"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:fontFamily="@font/andika"
                            android:text="Detail"
                            android:textAlignment="viewEnd" />

                    </LinearLayout>

                    <com.mikhaellopez.circularimageview.CircularImageView
                        android:id="@+id/imageprofiledokter"
                        android:layout_width="150dp"
                        android:layout_height="180dp"
                        android:src="@drawable/ic_man"
                        app:civ_border_color="#1fc8c6"
                        app:civ_shadow="true"
                        app:civ_shadow_color="#3f51b5"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toTopOf="parent"
                        app:srcCompat="@drawable/ic_arrow_right" />

                    <TextView
                        android:id="@+id/tv_profilenamapasien"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:fontFamily="@font/andika"
                        android:text="Nama"
                        android:textColor="#000000"
                        android:textSize="18sp"
                        app:layout_constraintStart_toEndOf="@+id/imageprofiledokter"
                        app:layout_constraintTop_toTopOf="parent" />

                </androidx.constraintlayout.widget.ConstraintLayout>

            </androidx.constraintlayout.widget.ConstraintLayout>

            <androidx.constraintlayout.widget.ConstraintLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_marginTop="-80dp"
                android:background="@drawable/corner_top_noborder_moreradius">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="20dp"
                    android:layout_marginTop="10dp"
                    android:layout_marginEnd="20dp"
                    android:orientation="vertical"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent">

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="10dp"
                        android:orientation="horizontal">

                        <LinearLayout
                            android:id="@+id/layoutMastePasien"
                            android:layout_width="match_parent"
                            android:layout_height="150dp"
                            android:layout_marginLeft="5dp"
                            android:layout_marginRight="5dp"
                            android:layout_weight="1"
                            android:background="@drawable/layout_bg"
                            android:orientation="vertical">

                            <ImageView
                                android:id="@+id/imageView281"
                                android:layout_width="match_parent"
                                android:layout_height="120dp"
                                app:srcCompat="@drawable/pasien" />

                            <TextView
                                android:id="@+id/txtMasterPasien"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:fontFamily="@font/andika"
                                android:text="Master Pasien"
                                android:textAlignment="center"
                                android:textColor="#000000" />

                        </LinearLayout>

                        <LinearLayout
                            android:id="@+id/layoutMasterDokter"
                            android:layout_width="match_parent"
                            android:layout_height="150dp"
                            android:layout_marginLeft="5dp"
                            android:layout_marginRight="5dp"
                            android:layout_weight="1"
                            android:background="@drawable/layout_bg"
                            android:orientation="vertical">

                            <ImageView
                                android:id="@+id/imageView272"
                                android:layout_width="match_parent"
                                android:layout_height="120dp"
                                android:paddingTop="10dp"
                                android:paddingBottom="10dp"
                                app:srcCompat="@drawable/doctor" />

                            <TextView
                                android:id="@+id/txtMasterDokter"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:fontFamily="@font/andika"
                                android:text="Master Dokter"
                                android:textAlignment="center"
                                android:textColor="#000000" />
                        </LinearLayout>
                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="10dp"
                        android:orientation="horizontal">

                        <LinearLayout
                            android:id="@+id/layoutMasterPenyakit"
                            android:layout_width="match_parent"
                            android:layout_height="150dp"
                            android:layout_marginLeft="5dp"
                            android:layout_marginRight="5dp"
                            android:layout_weight="1"
                            android:background="@drawable/layout_bg"
                            android:orientation="vertical">

                            <ImageView
                                android:id="@+id/imageView283"
                                android:layout_width="match_parent"
                                android:layout_height="120dp"
                                android:paddingTop="20dp"
                                android:paddingBottom="10dp"
                                app:srcCompat="@drawable/disease" />

                            <TextView
                                android:id="@+id/txtMasterPenyakit"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:fontFamily="@font/andika"
                                android:text="Master Penyakit"
                                android:textAlignment="center"
                                android:textColor="#000000" />

                        </LinearLayout>

                        <LinearLayout
                            android:id="@+id/layoutMasterSubs"
                            android:layout_width="match_parent"
                            android:layout_height="150dp"
                            android:layout_marginLeft="5dp"
                            android:layout_marginRight="5dp"
                            android:layout_weight="1"
                            android:background="@drawable/layout_bg"
                            android:orientation="vertical">

                            <ImageView
                                android:id="@+id/imageView274"
                                android:layout_width="match_parent"
                                android:layout_height="120dp"
                                android:paddingTop="10dp"
                                android:paddingBottom="10dp"
                                app:srcCompat="@drawable/subscribe" />

                            <TextView
                                android:id="@+id/txtMasterSubs"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:fontFamily="@font/andika"
                                android:text="Master Subscription"
                                android:textAlignment="center"
                                android:textColor="#000000" />
                        </LinearLayout>
                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="10dp"
                        android:orientation="horizontal">

                        <LinearLayout
                            android:id="@+id/layoutMasterBerita"
                            android:layout_width="match_parent"
                            android:layout_height="150dp"
                            android:layout_marginLeft="5dp"
                            android:layout_marginRight="5dp"
                            android:layout_weight="1"
                            android:background="@drawable/layout_bg"
                            android:orientation="vertical">

                            <ImageView
                                android:id="@+id/imageView276"
                                android:layout_width="match_parent"
                                android:layout_height="120dp"
                                android:paddingTop="20dp"
                                android:paddingBottom="10dp"
                                app:srcCompat="@drawable/news" />

                            <TextView
                                android:id="@+id/txtMasterBerita"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:fontFamily="@font/andika"
                                android:text="Master Berita"
                                android:textAlignment="center"
                                android:textColor="#000000" />
                        </LinearLayout>

                        <LinearLayout
                            android:id="@+id/layoutMasterReport"
                            android:layout_width="match_parent"
                            android:layout_height="150dp"
                            android:layout_marginLeft="5dp"
                            android:layout_marginRight="5dp"
                            android:layout_weight="1"
                            android:background="@drawable/layout_bg"
                            android:orientation="vertical">

                            <ImageView
                                android:id="@+id/imageView285"
                                android:layout_width="match_parent"
                                android:layout_height="120dp"
                                android:paddingTop="20dp"
                                android:paddingBottom="10dp"
                                app:srcCompat="@drawable/report" />

                            <TextView
                                android:id="@+id/txtMasterReport"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:fontFamily="@font/andika"
                                android:text="Laporan"
                                android:textAlignment="center"
                                android:textColor="#000000" />

                        </LinearLayout>
                    </LinearLayout>


                    <LinearLayout
                        android:id="@+id/layoutRegistAdmin"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="10dp"
                        android:orientation="horizontal">

                        <LinearLayout
                            android:id="@+id/layoutMasterAdmin"
                            android:layout_width="match_parent"
                            android:layout_height="150dp"
                            android:layout_marginLeft="5dp"
                            android:layout_marginRight="5dp"
                            android:layout_weight="1"
                            android:background="@drawable/layout_bg"
                            android:orientation="vertical">

                            <ImageView
                                android:id="@+id/imageView2851"
                                android:layout_width="match_parent"
                                android:layout_height="120dp"
                                android:paddingTop="20dp"
                                android:paddingBottom="20dp"
                                app:srcCompat="@drawable/admin" />

                            <TextView
                                android:id="@+id/txtMasterAdmin"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:fontFamily="@font/andika"
                                android:text="Daftar Admin"
                                android:textAlignment="center"
                                android:textColor="#000000" />

                        </LinearLayout>

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="150dp"
                            android:layout_marginLeft="5dp"
                            android:layout_marginRight="5dp"
                            android:layout_weight="1"
                            android:background="@drawable/layout_bg"
                            android:orientation="vertical"
                            android:visibility="invisible">

                            <ImageView
                                android:id="@+id/imageView2761"
                                android:layout_width="match_parent"
                                android:layout_height="120dp"
                                app:srcCompat="@drawable/free_hospital_waiting_room_illustration_svg" />

                            <TextView
                                android:id="@+id/textView3761"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:fontFamily="@font/andika"
                                android:text="TextView"
                                android:textAlignment="center"
                                android:textColor="#000000" />
                        </LinearLayout>

                    </LinearLayout>


                </LinearLayout>

            </androidx.constraintlayout.widget.ConstraintLayout>
        </LinearLayout>
    </ScrollView>
</androidx.constraintlayout.widget.ConstraintLayout>